<h1>Signin Page</h1>

<%= form_with url: login_path do |form| %>

  <div class="actions">
    <p>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </p>

    <p>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </p>

    <%= form.submit "Login" %>
  </div>

<% end %>

