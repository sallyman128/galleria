<p id="notice"><%= notice %></p>

<h1>Paintings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Country</th>
    </tr>
  </thead>

  <tbody>
    <% @paintings.each do |painting| %>
      <tr>
        <td><%= painting.title %></td>
        <td><%= painting.artist_name %></td>
        <td><%= painting.country %></td>
        <td><%= link_to 'Show', painting %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

